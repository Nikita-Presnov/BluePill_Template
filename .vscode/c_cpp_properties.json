{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/CMSIS/STM32F1xx/Include",
                "${workspaceFolder}/CMSIS/Include",
                "${workspaceFolder}/PeripheralLib/Inc",
                "${workspaceFolder}/USBLib"
            ],
            "defines": [
                "STM32F103xB",
                "USE_FULL_LL_DRIVER"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-arm",
            "compilerArgs": [
                "-O2",
                "-Wall",
                "-Wextra",
                "-Warray-bounds",
                "-lcc",
                "-lc",
                "-lm",
                "-MMD",
                "-MP",
                "-mthumb",
                "-mlittle-endian",
                "-mthumb-interwork"
            ]
        }
    ],
    "version": 4
}